#ifndef RS232_H_
#define RS232_H_

#include "Global.h"

struct RS232 {
	unsigned char data[50];
	unsigned char parity;
	struct Queue* sendPackets;
	struct Queue* receivePackets;
};

enum msgType {
	STRING=0, CMD=1, TXT=2, AUDIO=3
};
struct RS232 initRS232();
int send(void*, msgType);
#endif /* RS232_H_ */
