#ifndef PACKET_H_
#define PACKET_H_
#include "Global.h"
struct Packet {
	unsigned char* data;
	int data_size;
	enum msgType* type;
	int isHead;
	int isEnd;
};

struct Packet* initPacket();
struct Packet** encodeString(char*);
#endif /* PACKET_H_ */
